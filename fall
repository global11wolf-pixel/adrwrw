local ModuleLoader = {}
ModuleLoader.LoadedModules = {}
ModuleLoader.ModuleCache = {}

function ModuleLoader:LoadModule(moduleName, moduleURL, isLocal, libraryInstance)
    if self.LoadedModules[moduleName] then
        return self.LoadedModules[moduleName]
    end
    
    local moduleCode = nil
    
    if isLocal then
        moduleCode = self:GetLocalModule(moduleName)
    else
        local success, result = pcall(function()
            return game:HttpGet(moduleURL)
        end)
        
        if not success or not result then
            warn("Не удалось загрузить модуль: " .. moduleName)
            return nil
        end
        
        moduleCode = result
    end
    
    if not moduleCode then
        warn("Модуль " .. moduleName .. " не найден")
        return nil
    end
    
    local compiled, compileError = loadstring(moduleCode)
    if not compiled then
        warn("Ошибка компиляции модуля " .. moduleName .. ": " .. tostring(compileError))
        return nil
    end
    
    local execSuccess, moduleResult = pcall(function()
        local moduleFunc = compiled()
        if type(moduleFunc) == "function" then
            return moduleFunc(libraryInstance)
        else
            return moduleFunc
        end
    end)
    
    if not execSuccess then
        warn("Ошибка выполнения модуля " .. moduleName .. ": " .. tostring(moduleResult))
        return nil
    end
    
    self.LoadedModules[moduleName] = moduleResult
    return moduleResult
end

function ModuleLoader:GetLocalModule(moduleName)
    return nil
end

_G.ModuleLoader = ModuleLoader

