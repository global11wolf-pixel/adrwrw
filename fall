                OpenedTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                OpenedTab.BackgroundTransparency = 1
                OpenedTab.BorderColor3 = Color3.fromRGB(0, 0, 0)
                OpenedTab.BorderSizePixel = 0
                OpenedTab.Text = "Tab 1"
                OpenedTab.TextColor3 = Color3.fromRGB(255, 255, 255)
                OpenedTab.TextSize = 16
                OpenedTab.Font = Enum.Font.SourceSansBold
                OpenedTab.AnchorPoint = Vector2.new(0, 0)
                OpenedTab.Parent = ElementContainer

                local Settings = Instance.new("Frame")
                Settings.Name = "Settings"
                Settings.Size = UDim2.new(0, 35, 0, 35)
                Settings.Position = UDim2.new(0.9140238165855408, 0, 0.008333333767950535, 0)
                Settings.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                Settings.BackgroundTransparency = 1
                Settings.BorderColor3 = Color3.fromRGB(0, 0, 0)
                Settings.BorderSizePixel = 0
                Settings.AnchorPoint = Vector2.new(0, 0)

                    local Tab3_2 = Instance.new("TextButton")
                    Tab3_2.Name = "Tab3"
                    Tab3_2.Size = UDim2.new(0, 35, 0, 35)
                    Tab3_2.Position = UDim2.new(0.1594555675983429, 0, 0.14464294910430908, 0)
                    Tab3_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                    Tab3_2.BackgroundTransparency = 1
                    Tab3_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
                    Tab3_2.BorderSizePixel = 0
                    Tab3_2.Text = ""
                    Tab3_2.TextColor3 = Color3.fromRGB(0, 0, 0)
                    Tab3_2.TextSize = 14
                    Tab3_2.Font = Enum.Font.SourceSans
                    Tab3_2.AnchorPoint = Vector2.new(0, 0)

                        local UICorner_29 = Instance.new("UICorner")
                        UICorner_29.Parent = Tab3_2
                    Tab3_2.Parent = Settings

                    local UICorner_30 = Instance.new("UICorner")
                    UICorner_30.Parent = Settings
                Settings.Parent = ElementContainer
            ElementContainer.Parent = MainFrame_2

            local Line_10 = Instance.new("Frame")
            Line_10.Name = "Line"
            Line_10.Size = UDim2.new(0, 450, 0, 1)
            Line_10.Position = UDim2.new(0.22945205867290497, 0, 0.11075744777917862, 0)
            Line_10.BackgroundColor3 = Color3.fromRGB(161, 161, 161)
            Line_10.BackgroundTransparency = 0.699999988079071
            Line_10.BorderColor3 = Color3.fromRGB(0, 0, 0)
            Line_10.BorderSizePixel = 0
            Line_10.AnchorPoint = Vector2.new(0, 0)
            Line_10.Parent = MainFrame_2
        MainFrame_2.Parent = UI
    UI.Parent = ScreenGui

ScreenGui.Parent = Player:WaitForChild("PlayerGui")

-- // ЛОГИКА UI
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")

-- Берём второй UI (MainFrame_2) как основной
local main = MainFrame_2
local tabZone = main:FindFirstChild("TabZone")
local elementContainer = main:FindFirstChild("ElementContainer")
local openedTabLabel = elementContainer and elementContainer:FindFirstChild("OpenedTab")

local tab1Btn = tabZone and tabZone:FindFirstChild("Tab1(selected)")
local tab2Btn = tabZone and tabZone:FindFirstChild("Tab2(Mouse Targeted)")
local tab3Btn = tabZone and tabZone:FindFirstChild("Tab3")

local elementZone = elementContainer and elementContainer:FindFirstChild("ElementZone")
local section1 = elementZone and elementZone:FindFirstChild("Section1")
local section2 = elementZone and elementZone:FindFirstChild("Section2")
local section3 = elementZone and elementZone:FindFirstChild("Section3")

local toggle1Zone = section1 and section1:FindFirstChild("Toggle 1Zone")
local toggle1 = toggle1Zone and toggle1Zone:FindFirstChild("Toggle")
local toggle1Mover = toggle1 and toggle1:FindFirstChild("ToggleMover")

local toggle2Zone = section1 and section1:FindFirstChild("Toggle 2Zone(enabled)")
local toggle2 = toggle2Zone and toggle2Zone:FindFirstChild("Toggle")
local toggle2Mover = toggle2 and toggle2:FindFirstChild("ToggleMover")

local buttonZone = section1 and section1:FindFirstChild("ButtonZone")
local button1 = buttonZone and buttonZone:FindFirstChild("TextButton")
local button2
if buttonZone then
    for _, child in ipairs(buttonZone:GetChildren()) do
        if child:IsA("TextButton") and child ~= button1 then
            button2 = child
            break
        end
    end
end

local sliderZone = section2 and section2:FindFirstChild("SliderZone")
local sliderLine = sliderZone and sliderZone:FindFirstChild("SliderLine")
local sliderKnob = sliderLine and sliderLine:FindFirstChild("Frame")
local sliderValueFrame = sliderZone and sliderZone:FindFirstChild("SliderValue")
local sliderValueLabel = sliderValueFrame and sliderValueFrame:FindFirstChildOfClass("TextLabel")

local bindZone = section2 and section2:FindFirstChild("BindZone")
local bindFrame = bindZone and bindZone:FindFirstChild("Bind")
local bindKeyLabel = bindFrame and bindFrame:FindFirstChild("BindName")

local inputZone = section2 and section2:FindFirstChild("InputZone")
local inputFrame = inputZone and inputZone:FindFirstChild("Input")
local inputValueLabel = inputFrame and inputFrame:FindFirstChild("InputValue")

local dropdownZone = section3 and section3:FindFirstChild("DropdownZone")
local dropdownButton = dropdownZone and dropdownZone:FindFirstChild("DropdownButton")
local dropdownSelected = dropdownButton and dropdownButton:FindFirstChild("Selected")
local dropdownArrow = dropdownButton and dropdownButton:FindFirstChild("Arrow")

local notifiGui = ScreenGui:FindFirstChild("Notifi")
local notifiList = notifiGui and notifiGui:FindFirstChild("NotifiList")
local notificationTemplate = notifiList and notifiList:FindFirstChild("Notification1")

local hotKeysGui = ScreenGui:FindFirstChild("HotKeys")
local hotKeysMain = hotKeysGui and hotKeysGui:FindFirstChild("MainFrame")
local hotKey1 = hotKeysMain and hotKeysMain:FindFirstChild("HotKey1")
local hotKey1KeyLabel = hotKey1 and hotKey1:FindFirstChild("Key")

-- // Табы
local function setTab(index)
    if openedTabLabel then
        openedTabLabel.Text = "Tab " .. tostring(index)
    end

    local function setVisual(btn, active)
        if not btn then return end
        local logo = btn:FindFirstChild("TabLogo")
        local name = btn:FindFirstChild("TabName")
        if active then
            btn.BackgroundTransparency = 0.9
            if logo then logo.ImageColor3 = Color3.fromRGB(150, 17, 255) end
            if name then name.TextColor3 = Color3.fromRGB(150, 17, 255) end
        else
            btn.BackgroundTransparency = 1
            if logo then logo.ImageColor3 = Color3.fromRGB(161, 161, 161) end
            if name then name.TextColor3 = Color3.fromRGB(161, 161, 161) end
        end
    end

    setVisual(tab1Btn, index == 1)
    setVisual(tab2Btn, index == 2)
    setVisual(tab3Btn, index == 3)
end

setTab(1)

if tab1Btn then
    tab1Btn.MouseButton1Click:Connect(function() setTab(1) end)
end
if tab2Btn then
    tab2Btn.MouseButton1Click:Connect(function() setTab(2) end)
end
if tab3Btn then
    tab3Btn.MouseButton1Click:Connect(function() setTab(3) end)
end

-- // Тогглы
local function setupToggle(toggleFrame, mover)
    if not (toggleFrame and mover) then return end
    local enabled = false
    local offColor = Color3.fromRGB(29, 29, 29)
    local onColor = Color3.fromRGB(150, 17, 255)

    local function apply()
        toggleFrame.BackgroundColor3 = enabled and onColor or offColor
        local goal = {
            Position = enabled and UDim2.new(0.6, 0, 0.125, 0) or UDim2.new(0, 2, 0.125, 0),
        }
        TweenService:Create(mover, TweenInfo.new(0.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), goal):Play()
    end

    apply()

    toggleFrame.MouseButton1Click:Connect(function()
        enabled = not enabled
        apply()
        print(toggleFrame.Name .. " =", enabled)
    end)
end

setupToggle(toggle1, toggle1Mover)
setupToggle(toggle2, toggle2Mover)

-- // Кнопки
if button1 then
    button1.MouseButton1Click:Connect(function()
        print("Button 1 clicked")
    end)
end

if button2 then
    button2.MouseButton1Click:Connect(function()
        print("Button 2 clicked")
    end)
end

-- // Слайдер 0-100
if sliderLine and sliderKnob and sliderValueLabel then
    local dragging = false

    local function setSliderFromX(x)
        local absPos = sliderLine.AbsolutePosition.X
        local absSize = sliderLine.AbsoluteSize.X
        local alpha = math.clamp((x - absPos) / absSize, 0, 1)
        local value = math.floor(alpha * 100)
        sliderValueLabel.Text = tostring(value)

        local knobSize = sliderKnob.AbsoluteSize.X
        local goalX = alpha * (absSize - knobSize)
        sliderKnob.Position = UDim2.new(0, goalX, sliderKnob.Position.Y.Scale, sliderKnob.Position.Y.Offset)
    end

    sliderLine.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            setSliderFromX(input.Position.X)
        end
    end)

    sliderLine.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)

    UserInputService.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            setSliderFromX(input.Position.X)
        end
    end)
end

-- // Бинд клавиши
do
    if bindFrame and bindKeyLabel and hotKey1KeyLabel then
        local listening = false
        local currentKey = Enum.KeyCode.B

        local function setKey(keyCode)
            currentKey = keyCode
            bindKeyLabel.Text = keyCode.Name
            hotKey1KeyLabel.Text = keyCode.Name
        end

        setKey(currentKey)

        bindFrame.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                listening = true
                bindKeyLabel.Text = "..."
            end
        end)

        UserInputService.InputBegan:Connect(function(input, gp)
            if gp then return end
            if listening and input.UserInputType == Enum.UserInputType.Keyboard then
                listening = false
                if input.KeyCode ~= Enum.KeyCode.Unknown then
                    setKey(input.KeyCode)
                end
            elseif input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == currentKey then
                -- действие по бинду: скрыть/показать главное окно
                main.Visible = not main.Visible
            end
        end)
    end
end

-- // Input: просто пример смены текста
if inputFrame and inputValueLabel then
    inputFrame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            inputValueLabel.Text = "Clicked " .. os.time()
        end
    end)
end

-- // Dropdown
do
    if dropdownButton and dropdownSelected and dropdownArrow then
        local options = {"Goida", "Svo", "Zov", "228"}
        local opened = false

        -- Создаём внутренние кнопки опций
        for i, name in ipairs(options) do
            local opt = Instance.new("TextButton")
            opt.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
            opt.BorderSizePixel = 0
            opt.Size = UDim2.new(1, 0, 0, 18)
            opt.Position = UDim2.new(0, 0, 0, 24 + (i - 1) * 18)
            opt.Font = Enum.Font.SourceSans
            opt.TextColor3 = Color3.fromRGB(255, 255, 255)
            opt.TextSize = 12
            opt.Text = name
            opt.Parent = dropdownButton

            opt.MouseButton1Click:Connect(function()
                dropdownSelected.Text = name
                opened = false
                TweenService:Create(dropdownButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                    Size = UDim2.new(0, 111, 0, 24),
                }):Play()
                TweenService:Create(dropdownArrow, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                    Rotation = 0,
                }):Play()
            end)
        end

        local closedSize = dropdownButton.Size
        local openSize = UDim2.new(0, 111, 0, 24 + #options * 18)

        local function toggleDropdown()
            opened = not opened
            TweenService:Create(dropdownButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                Size = opened and openSize or closedSize,
            }):Play()
            TweenService:Create(dropdownArrow, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                Rotation = opened and 180 or 0,
            }):Play()
        end

        dropdownButton.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                toggleDropdown()
            end
        end)
    end
end

-- // Нотификации
local function ShowNotification(text, duration)
    if not (notificationTemplate and notifiList) then return end
    duration = duration or 3

    local n = notificationTemplate:Clone()
    n.Visible = true
    n.Parent = notifiList

    local label = n:FindFirstChild("NotifiText", true)
    if label and label:IsA("TextLabel") then
        label.Text = text
    end

    n.Position = UDim2.new(0, 0, 1, 0)
    TweenService:Create(n, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        Position = UDim2.new(0, 0, 0.5, 0),
    }):Play()

    task.delay(duration, function()
        if n.Parent then
            TweenService:Create(n, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
                Position = UDim2.new(0, 0, 1, 0),
            }):Play()
            task.wait(0.3)
            if n then
                n:Destroy()
            end
        end
    end)
end

ShowNotification("PizdecWare UI Loaded", 2)

-- // Глобальный хоткей RightControl для скрытия/показа UI
UserInputService.InputBegan:Connect(function(input, gp)
    if gp then return end
    if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.RightControl then
        if main then
            main.Visible = not main.Visible
        end
    end
end)
