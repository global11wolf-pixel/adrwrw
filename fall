local function setupToggle(toggleFrame, mover)
    if not (toggleFrame and mover) then return end
    local enabled = false
    local offColor = Color3.fromRGB(29, 29, 29)
    local onColor = Color3.fromRGB(150, 17, 255)

    local function apply()
        toggleFrame.BackgroundColor3 = enabled and onColor or offColor
        local goal = {
            Position = enabled and UDim2.new(0.6, 0, 0.125, 0) or UDim2.new(0, 2, 0.125, 0),
        }
        TweenService:Create(mover, TweenInfo.new(0.15, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), goal):Play()
    end

    apply()

    toggleFrame.MouseButton1Click:Connect(function()
        enabled = not enabled
        apply()
        print(toggleFrame.Name .. " =", enabled)
    end)
end

setupToggle(UIData.elements.toggle1, UIData.elements.toggle1Mover)
setupToggle(UIData.elements.toggle2, UIData.elements.toggle2Mover)

if UIData.elements.button1 then
    UIData.elements.button1.MouseButton1Click:Connect(function()
        print("Button 1 clicked")
    end)
end

if UIData.elements.button2 then
    UIData.elements.button2.MouseButton1Click:Connect(function()
        print("Button 2 clicked")
    end)
end

if UIData.elements.sliderLine and UIData.elements.sliderKnob and UIData.elements.sliderValueLabel then
    UIData.elements.sliderDragging = false
    local sLine, sKnob, sLabel = UIData.elements.sliderLine, UIData.elements.sliderKnob, UIData.elements.sliderValueLabel

    sLine.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            UIData.elements.sliderDragging = true
            local x = input.Position.X
            local absPos = sLine.AbsolutePosition.X
            local absSize = sLine.AbsoluteSize.X
        local alpha = math.clamp((x - absPos) / absSize, 0, 1)
            sLabel.Text = tostring(math.floor(alpha * 100))
            sKnob.Position = UDim2.new(0, alpha * (absSize - sKnob.AbsoluteSize.X), sKnob.Position.Y.Scale, sKnob.Position.Y.Offset)
        end
    end)

    sLine.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            UIData.elements.sliderDragging = false
        end
    end)

    UserInputService.InputChanged:Connect(function(input)
        if UIData.elements.sliderDragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local x = input.Position.X
            local absPos = sLine.AbsolutePosition.X
            local absSize = sLine.AbsoluteSize.X
            local alpha = math.clamp((x - absPos) / absSize, 0, 1)
            sLabel.Text = tostring(math.floor(alpha * 100))
            sKnob.Position = UDim2.new(0, alpha * (absSize - sKnob.AbsoluteSize.X), sKnob.Position.Y.Scale, sKnob.Position.Y.Offset)
        end
    end)
end

do
    if UIData.elements.bindFrame and UIData.elements.bindKeyLabel and UIData.hotKey1KeyLabel then
        UIData.elements.bindListening = false
        UIData.elements.currentKey = Enum.KeyCode.B

        local function setKey(keyCode)
            UIData.elements.currentKey = keyCode
            UIData.elements.bindKeyLabel.Text = keyCode.Name
            UIData.hotKey1KeyLabel.Text = keyCode.Name
        end

        setKey(UIData.elements.currentKey)

        UIData.elements.bindFrame.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                UIData.elements.bindListening = true
                UIData.elements.bindKeyLabel.Text = "..."
            end
        end)

        UserInputService.InputBegan:Connect(function(input, gp)
            if gp then return end
            if UIData.elements.bindListening and input.UserInputType == Enum.UserInputType.Keyboard then
                UIData.elements.bindListening = false
                if input.KeyCode ~= Enum.KeyCode.Unknown then
                    setKey(input.KeyCode)
                end
            elseif input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == UIData.elements.currentKey then
                UIData.main.Visible = not UIData.main.Visible
            end
        end)
    end
end

if UIData.elements.inputFrame and UIData.elements.inputValueLabel then
    UIData.elements.inputFrame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            UIData.elements.inputValueLabel.Text = "Clicked " .. os.time()
        end
    end)
end

do
    if UIData.elements.dropdownButton and UIData.elements.dropdownSelected and UIData.elements.dropdownArrow then
        local options = {"Goida", "Svo", "Zov", "228"}
        local opened = false

        for i, name in ipairs(options) do
            local opt = Instance.new("TextButton")
            opt.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
            opt.BorderSizePixel = 0
            opt.Size = UDim2.new(1, 0, 0, 18)
            opt.Position = UDim2.new(0, 0, 0, 24 + (i - 1) * 18)
            opt.Font = Enum.Font.SourceSans
            opt.TextColor3 = Color3.fromRGB(255, 255, 255)
            opt.TextSize = 12
            opt.Text = name
            opt.Parent = UIData.elements.dropdownButton

            opt.MouseButton1Click:Connect(function()
                UIData.elements.dropdownSelected.Text = name
                UIData.elements.dropdownOpened = false
                TweenService:Create(UIData.elements.dropdownButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                    Size = UDim2.new(0, 111, 0, 24),
                }):Play()
                TweenService:Create(UIData.elements.dropdownArrow, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                    Rotation = 0,
                }):Play()
            end)
        end

        local closedSize = UIData.elements.dropdownButton.Size
        local openSize = UDim2.new(0, 111, 0, 24 + #options * 18)
        UIData.elements.dropdownOpened = false

        local function toggleDropdown()
            UIData.elements.dropdownOpened = not UIData.elements.dropdownOpened
            TweenService:Create(UIData.elements.dropdownButton, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                Size = UIData.elements.dropdownOpened and openSize or closedSize,
            }):Play()
            TweenService:Create(UIData.elements.dropdownArrow, TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
                Rotation = UIData.elements.dropdownOpened and 180 or 0,
            }):Play()
        end

        UIData.elements.dropdownButton.InputBegan:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                toggleDropdown()
            end
        end)
    end
end

local function ShowNotification(text, duration)
    if not (UIData.notificationTemplate and UIData.notifiList) then return end
    duration = duration or 3

    local n = UIData.notificationTemplate:Clone()
    n.Visible = true
    n.Parent = UIData.notifiList

    local label = n:FindFirstChild("NotifiText", true)
    if label and label:IsA("TextLabel") then
        label.Text = text
    end

    n.Position = UDim2.new(0, 0, 1, 0)
    TweenService:Create(n, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
        Position = UDim2.new(0, 0, 0.5, 0),
    }):Play()

    task.delay(duration, function()
        if n.Parent then
            TweenService:Create(n, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.In), {
                Position = UDim2.new(0, 0, 1, 0),
            }):Play()
            task.wait(0.3)
            if n then
                n:Destroy()
            end
        end
    end)
end

ShowNotification("PizdecWare UI Loaded", 2)

UserInputService.InputBegan:Connect(function(input, gp)
    if gp then return end
    if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.RightControl then
        if UIData.main then
            UIData.main.Visible = not UIData.main.Visible
        end
    end
end)

local Library = {
    UI = UIData,
    MainFrame = MainFrame_2,
    TabZone = UIData.tabZone,
    ElementContainer = UIData.elementContainer,
    ElementZone = UIData.elementZone,
    OpenedTab = UIData.openedTabLabel,
    Tabs = {
        Tab1 = UIData.tabs[1],
        Tab2 = UIData.tabs[2],
        Tab3 = UIData.tabs[3],
        Tab4 = UIData.tabs[4],
        Tab5 = UIData.tabs[5]
    },
    Sections = {
        Section1 = UIData.sections[1],
        Section2 = UIData.sections[2],
        Section3 = UIData.sections[3]
    },
    Elements = UIData.elements,
    ShowNotification = ShowNotification,
    setTab = setTab,
    TweenService = TweenService,
    UserInputService = UserInputService,
    ModuleLoader = ModuleLoader
}

function Library:LoadModule(moduleName, moduleURL)
    return ModuleLoader:LoadModule(moduleName, moduleURL, false, self)
end

function Library:LoadLocalModule(moduleName, moduleCode)
    if ModuleLoader.LoadedModules[moduleName] then
        return ModuleLoader.LoadedModules[moduleName]
    end
    
    local compiled, compileError = loadstring(moduleCode)
    if not compiled then
        warn("Ошибка компиляции локального модуля " .. moduleName .. ": " .. tostring(compileError))
        return nil
    end
    
    local execSuccess, moduleResult = pcall(function()
        local moduleFunc = compiled()
        if type(moduleFunc) == "function" then
            return moduleFunc(self)
        else
            return moduleFunc
        end
    end)
    
    if not execSuccess then
        warn("Ошибка выполнения локального модуля " .. moduleName .. ": " .. tostring(moduleResult))
        return nil
    end
    
    ModuleLoader.LoadedModules[moduleName] = moduleResult
    return moduleResult
end

function Library:CreateTab(name, index)
    if not UIData.tabZone then return nil end
    
    local tabCount = #UIData.tabs
    local tabIndex = index or (tabCount + 1)
    
    local baseY = 0.0535714291036129
    local tabSpacing = 0.0892857201397419
    local yPos = baseY + (tabIndex - 1) * tabSpacing
    
    local tabButton = Instance.new("TextButton")
    tabButton.Name = "Tab" .. tabIndex
    tabButton.Size = UDim2.new(0, 120, 0, 25)
    tabButton.Position = UDim2.new(0.06428571790456772, 0, yPos, 0)
    tabButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    tabButton.BackgroundTransparency = tabIndex == 1 and 0.9 or 0.95
    tabButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
    tabButton.BorderSizePixel = 0
    tabButton.Text = ""
    tabButton.TextColor3 = Color3.fromRGB(0, 0, 0)
    tabButton.TextSize = 14
    tabButton.Font = Enum.Font.SourceSans
    tabButton.AnchorPoint = Vector2.new(0, 0)
    
    local corner = Instance.new("UICorner")
    corner.Parent = tabButton
    
    local logo = Instance.new("ImageLabel")
    logo.Name = "TabLogo"
    logo.Size = UDim2.new(0, 14, 0, 14)
    logo.Position = UDim2.new(0.06666667014360428, 0, 0.20000000298023224, 0)
    logo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    logo.BackgroundTransparency = 1
    logo.BorderColor3 = Color3.fromRGB(0, 0, 0)
    logo.BorderSizePixel = 0
    logo.Image = "rbxassetid://12614416478"
    logo.ImageColor3 = tabIndex == 1 and Color3.fromRGB(150, 17, 255) or Color3.fromRGB(161, 161, 161)
    logo.AnchorPoint = Vector2.new(0, 0)
    logo.Parent = tabButton
    
    local tabNameLabel = Instance.new("TextLabel")
    tabNameLabel.Name = "TabName"
    tabNameLabel.Size = UDim2.new(0, 89, 0, 23)
    tabNameLabel.Position = UDim2.new(0.25, 0, -0.03999878093600273, 0)
    tabNameLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    tabNameLabel.BackgroundTransparency = 1
    tabNameLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
    tabNameLabel.BorderSizePixel = 0
    tabNameLabel.Text = name or "Tab " .. tabIndex
    tabNameLabel.TextColor3 = tabIndex == 1 and Color3.fromRGB(150, 17, 255) or Color3.fromRGB(161, 161, 161)
    tabNameLabel.TextSize = 14
    tabNameLabel.Font = Enum.Font.SourceSansBold
    tabNameLabel.AnchorPoint = Vector2.new(0, 0)
    tabNameLabel.Parent = tabButton
    
    tabButton.Parent = UIData.tabZone
    
    tabButton.MouseButton1Click:Connect(function()
        setTab(tabIndex)
    end)
    
    UIData.tabs[tabIndex] = tabButton
    return tabButton
end

function Library:CreateSection(tabIndex, sectionName, position)
    if not UIData.elementZone then return nil end
    
    local sectionCount = #UIData.sections
    local sectionIndex = sectionCount + 1
    
    local section = Instance.new("Frame")
    section.Name = "Section" .. sectionIndex
    section.Size = UDim2.new(0, 210, 0, 100)
    section.Position = position or UDim2.new(0.5331807732582092, 0, 0.06666668504476547, 0)
    section.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
    section.BackgroundTransparency = 0.6000000238418579
    section.BorderColor3 = Color3.fromRGB(0, 0, 0)
    section.BorderSizePixel = 0
    section.AnchorPoint = Vector2.new(0, 0)
    
    local sectionNameLabel = Instance.new("TextLabel")
    sectionNameLabel.Name = "SectionName"
    sectionNameLabel.Size = UDim2.new(0, 78, 0, 19)
    sectionNameLabel.Position = UDim2.new(0.037837836891412735, 0, -0.1933334320783615, 0)
    sectionNameLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    sectionNameLabel.BackgroundTransparency = 1
    sectionNameLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
    sectionNameLabel.BorderSizePixel = 0
    sectionNameLabel.Text = sectionName or "Section " .. sectionIndex
    sectionNameLabel.TextColor3 = Color3.fromRGB(161, 161, 161)
    sectionNameLabel.TextSize = 12
    sectionNameLabel.Font = Enum.Font.SourceSansBold
    sectionNameLabel.AnchorPoint = Vector2.new(0, 0)
    sectionNameLabel.Parent = section
    
    local corner = Instance.new("UICorner")
    corner.Parent = section
    
    local stroke = Instance.new("UIStroke")
    stroke.Parent = section
    
    section.Parent = UIData.elementZone
    UIData.sections[sectionIndex] = section
    return section
end

local function getNextElementPosition(section)
    local maxY = 0
    for _, child in ipairs(section:GetChildren()) do
        if child:IsA("Frame") and child.Name:match("Zone") then
            local yPos = child.Position.Y.Offset
            local height = child.Size.Y.Offset
            local totalY = yPos + height
            if totalY > maxY then
                maxY = totalY
            end
        end
    end
    return maxY
end

function Library:CreateToggle(section, name, callback, defaultState)
    if not section then return nil end
    
    defaultState = defaultState or false
    
    local yOffset = getNextElementPosition(section)
    
    local toggleZone = Instance.new("Frame")
    toggleZone.Name = name .. "Zone"
    toggleZone.Size = UDim2.new(0, 210, 0, 30)
    toggleZone.Position = UDim2.new(0, 0, 0, yOffset)
    toggleZone.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    toggleZone.BackgroundTransparency = 1
    toggleZone.BorderColor3 = Color3.fromRGB(0, 0, 0)
    toggleZone.BorderSizePixel = 0
    toggleZone.AnchorPoint = Vector2.new(0, 0)
    
    local toggleName = Instance.new("TextLabel")
    toggleName.Name = "ToggleName"
    toggleName.Size = UDim2.new(0, 79, 0, 30)
    toggleName.Position = UDim2.new(0.07027027010917664, 0, 0, 0)
    toggleName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    toggleName.BackgroundTransparency = 1
    toggleName.BorderColor3 = Color3.fromRGB(0, 0, 0)
    toggleName.BorderSizePixel = 0
    toggleName.Text = name
    toggleName.TextColor3 = Color3.fromRGB(161, 161, 161)
    toggleName.TextSize = 12
    toggleName.Font = Enum.Font.SourceSansBold
    toggleName.AnchorPoint = Vector2.new(0, 0)
    toggleName.Parent = toggleZone
    
    local line = Instance.new("Frame")
    line.Name = "Line"
    line.Size = UDim2.new(0, 190, 0, 1)
    line.Position = UDim2.new(0.054054055362939835, 0, 0.9937683343887329, 0)
    line.BackgroundColor3 = Color3.fromRGB(161, 161, 161)
    line.BackgroundTransparency = 0.949999988079071
    line.BorderColor3 = Color3.fromRGB(0, 0, 0)
    line.BorderSizePixel = 0
    line.AnchorPoint = Vector2.new(0, 0)
    line.Parent = toggleZone
    
    local toggle = Instance.new("TextButton")
    toggle.Name = "Toggle"
    toggle.Size = UDim2.new(0, 30, 0, 16)
    toggle.Position = UDim2.new(0.7610037922859192, 0, 0.23333333432674408, 0)
    toggle.BackgroundColor3 = defaultState and Color3.fromRGB(150, 17, 255) or Color3.fromRGB(29, 29, 29)
    toggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
    toggle.BorderSizePixel = 0
    toggle.Text = ""
    toggle.TextColor3 = Color3.fromRGB(0, 0, 0)
    toggle.TextSize = 14
    toggle.Font = Enum.Font.SourceSans
    toggle.AnchorPoint = Vector2.new(0, 0)
    
    local corner = Instance.new("UICorner")
    corner.Parent = toggle
    
    local mover = Instance.new("TextButton")
    mover.Name = "ToggleMover"
    mover.Size = UDim2.new(0, 12, 0, 12)
    mover.Position = defaultState and UDim2.new(0.44999998807907104, 2, 0.125, 0) or UDim2.new(0, 2, 0.125, 0)
    mover.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    mover.BorderColor3 = Color3.fromRGB(0, 0, 0)
    mover.BorderSizePixel = 0
    mover.Text = ""
    mover.TextColor3 = Color3.fromRGB(0, 0, 0)
    mover.TextSize = 14
    mover.Font = Enum.Font.SourceSans
    mover.AnchorPoint = Vector2.new(0, 0)
    
    local moverCorner = Instance.new("UICorner")
    moverCorner.Parent = mover
    mover.Parent = toggle
    toggle.Parent = toggleZone
    
    toggle.MouseButton1Click:Connect(function()
        defaultState = not defaultState
        toggle.BackgroundColor3 = defaultState and Color3.fromRGB(150, 17, 255) or Color3.fromRGB(29, 29, 29)
        mover.Position = defaultState and UDim2.new(0.44999998807907104, 2, 0.125, 0) or UDim2.new(0, 2, 0.125, 0)
        if callback then callback(defaultState) end
    end)
    
    toggleZone.Parent = section
    return {Zone = toggleZone, Toggle = toggle, State = defaultState}
end

function Library:CreateButton(section, name, callback)
    if not section then return nil end
    
    local yOffset = getNextElementPosition(section)
    
    local buttonZone = Instance.new("Frame")
    buttonZone.Name = name .. "Zone"
    buttonZone.Size = UDim2.new(0, 210, 0, 35)
    buttonZone.Position = UDim2.new(0, 0, 0, yOffset)
    buttonZone.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    buttonZone.BackgroundTransparency = 1
    buttonZone.BorderColor3 = Color3.fromRGB(0, 0, 0)
    buttonZone.BorderSizePixel = 0
    buttonZone.AnchorPoint = Vector2.new(0, 0)
    
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 90, 0, 25)
    button.Position = UDim2.new(0.06666667014360428, 0, 0.13809552788734436, 0)
    button.BackgroundColor3 = Color3.fromRGB(150, 17, 255)
    button.BorderColor3 = Color3.fromRGB(0, 0, 0)
    button.BorderSizePixel = 0
    button.Text = name
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.TextSize = 15
    button.Font = Enum.Font.SourceSansBold
    button.AnchorPoint = Vector2.new(0, 0)
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 5)
    corner.Parent = button
    button.Parent = buttonZone
    
    local line = Instance.new("Frame")
    line.Name = "Line"
    line.Size = UDim2.new(0, 190, 0, 1)
    line.Position = UDim2.new(0.054054055362939835, 0, 0.9937683343887329, 0)
    line.BackgroundColor3 = Color3.fromRGB(161, 161, 161)
    line.BackgroundTransparency = 0.949999988079071
    line.BorderColor3 = Color3.fromRGB(0, 0, 0)
    line.BorderSizePixel = 0
    line.AnchorPoint = Vector2.new(0, 0)
    line.Parent = buttonZone
    
    if callback then
        button.MouseButton1Click:Connect(callback)
    end
    
    buttonZone.Parent = section
    return button
end

function Library:CreateSlider(section, name, min, max, default, callback)
    if not section then return nil end
    
    min = min or 0
    max = max or 100
    default = default or min
    
    local yOffset = getNextElementPosition(section)
    
    local sliderZone = Instance.new("Frame")
    sliderZone.Name = name .. "Zone"
    sliderZone.Size = UDim2.new(0, 210, 0, 30)
    sliderZone.Position = UDim2.new(0, 0, 0, yOffset)
    sliderZone.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    sliderZone.BackgroundTransparency = 1
    sliderZone.BorderColor3 = Color3.fromRGB(0, 0, 0)
    sliderZone.BorderSizePixel = 0
    sliderZone.AnchorPoint = Vector2.new(0, 0)
    
    local sliderName = Instance.new("TextLabel")
    sliderName.Name = "SliderName"
    sliderName.Size = UDim2.new(0, 79, 0, 30)
    sliderName.Position = UDim2.new(0.05122259259223938, 0, -0.03333333507180214, 0)
    sliderName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    sliderName.BackgroundTransparency = 1
    sliderName.BorderColor3 = Color3.fromRGB(0, 0, 0)
    sliderName.BorderSizePixel = 0
    sliderName.Text = name
    sliderName.TextColor3 = Color3.fromRGB(255, 255, 255)
    sliderName.TextSize = 12
    sliderName.Font = Enum.Font.SourceSansBold
    sliderName.AnchorPoint = Vector2.new(0, 0)
    sliderName.Parent = sliderZone
    
    local line = Instance.new("Frame")
    line.Name = "Line"
    line.Size = UDim2.new(0, 190, 0, 1)
    line.Position = UDim2.new(0.054054055362939835, 0, 0.9937683343887329, 0)
    line.BackgroundColor3 = Color3.fromRGB(161, 161, 161)
    line.BackgroundTransparency = 0.949999988079071
    line.BorderColor3 = Color3.fromRGB(0, 0, 0)
    line.BorderSizePixel = 0
    line.AnchorPoint = Vector2.new(0, 0)
    line.Parent = sliderZone
    
    local sliderLine = Instance.new("Frame")
    sliderLine.Name = "SliderLine"
    sliderLine.Size = UDim2.new(0, 80, 0, 4)
    sliderLine.Position = UDim2.new(0.34169864654541016, 0, 0.4333333373069763, 0)
    sliderLine.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
    sliderLine.BorderColor3 = Color3.fromRGB(0, 0, 0)
    sliderLine.BorderSizePixel = 0
    sliderLine.AnchorPoint = Vector2.new(0, 0)
    
    local sliderCorner = Instance.new("UICorner")
    sliderCorner.Parent = sliderLine
    
    local sliderStroke = Instance.new("UIStroke")
    sliderStroke.Parent = sliderLine
    
    local percent = (default - min) / (max - min)
    local knob = Instance.new("Frame")
    knob.Size = UDim2.new(0, 10, 0, 10)
    knob.Position = UDim2.new(percent * 0.875, 0, -0.75, 0)
    knob.BackgroundColor3 = Color3.fromRGB(150, 17, 255)
    knob.BorderColor3 = Color3.fromRGB(0, 0, 0)
    knob.BorderSizePixel = 0
    knob.AnchorPoint = Vector2.new(0, 0)
    
    local knobCorner = Instance.new("UICorner")
    knobCorner.CornerRadius = UDim.new(1, 0)
    knobCorner.Parent = knob
    knob.Parent = sliderLine
    sliderLine.Parent = sliderZone
    
    local sliderValue = Instance.new("Frame")
    sliderValue.Name = "SliderValue"
    sliderValue.Size = UDim2.new(0, 30, 0, 12)
    sliderValue.Position = UDim2.new(0.7666666507720947, 0, 0.30000001192092896, 0)
    sliderValue.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
    sliderValue.BorderColor3 = Color3.fromRGB(161, 161, 161)
    sliderValue.BorderSizePixel = 0
    sliderValue.AnchorPoint = Vector2.new(0, 0)
    
    local valueStroke = Instance.new("UIStroke")
    valueStroke.Parent = sliderValue
    
    local valueLabel = Instance.new("TextLabel")
    valueLabel.Size = UDim2.new(0, 29, 0, 10)
    valueLabel.Position = UDim2.new(0.02702636644244194, 0, 0, 0)
    valueLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    valueLabel.BackgroundTransparency = 1
    valueLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
    valueLabel.BorderSizePixel = 0
    valueLabel.Text = tostring(default)
    valueLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    valueLabel.TextSize = 10
    valueLabel.Font = Enum.Font.SourceSans
    valueLabel.AnchorPoint = Vector2.new(0, 0)
    valueLabel.Parent = sliderValue
    sliderValue.Parent = sliderZone
    
    local dragging = false
    sliderLine.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
            local mousePos = UserInputService:GetMouseLocation()
            local sliderPos = sliderLine.AbsolutePosition
            local sliderSize = sliderLine.AbsoluteSize
            local relativeX = math.clamp((mousePos.X - sliderPos.X) / sliderSize.X, 0, 1)
            local value = math.floor(min + relativeX * (max - min))
            valueLabel.Text = tostring(value)
            knob.Position = UDim2.new(relativeX * 0.875, 0, -0.75, 0)
            if callback then callback(value) end
        end
    end)
    
    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)
    
    sliderZone.Parent = section
    return {Zone = sliderZone, Value = default, SetValue = function(val)
        val = math.clamp(val, min, max)
        local percent = (val - min) / (max - min)
        valueLabel.Text = tostring(val)
        knob.Position = UDim2.new(percent * 0.875, 0, -0.75, 0)
    end}
end

function Library:CreateBind(section, name, defaultKey, callback)
    if not section then return nil end
    
    local yOffset = getNextElementPosition(section)
    
    local bindZone = Instance.new("Frame")
    bindZone.Name = name .. "Zone"
    bindZone.Size = UDim2.new(0, 210, 0, 30)
    bindZone.Position = UDim2.new(0, 0, 0, yOffset)
    bindZone.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    bindZone.BackgroundTransparency = 1
    bindZone.BorderColor3 = Color3.fromRGB(0, 0, 0)
    bindZone.BorderSizePixel = 0
    bindZone.AnchorPoint = Vector2.new(0, 0)
    
    local bindName = Instance.new("TextLabel")
    bindName.Name = "BindName"
    bindName.Size = UDim2.new(0, 79, 0, 30)
    bindName.Position = UDim2.new(0.05122259259223938, 0, -0.03333333507180214, 0)
    bindName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    bindName.BackgroundTransparency = 1
    bindName.BorderColor3 = Color3.fromRGB(0, 0, 0)
    bindName.BorderSizePixel = 0
    bindName.Text = name
    bindName.TextColor3 = Color3.fromRGB(255, 255, 255)
    bindName.TextSize = 12
    bindName.Font = Enum.Font.SourceSansBold
    bindName.AnchorPoint = Vector2.new(0, 0)
    bindName.Parent = bindZone
    
    local line = Instance.new("Frame")
    line.Name = "Line"
    line.Size = UDim2.new(0, 190, 0, 1)
    line.Position = UDim2.new(0.054054055362939835, 0, 0.9937683343887329, 0)
    line.BackgroundColor3 = Color3.fromRGB(161, 161, 161)
    line.BackgroundTransparency = 0.949999988079071
    line.BorderColor3 = Color3.fromRGB(0, 0, 0)
    line.BorderSizePixel = 0
    line.AnchorPoint = Vector2.new(0, 0)
    line.Parent = bindZone
    
    local bindFrame = Instance.new("TextButton")
    bindFrame.Name = "Bind"
    bindFrame.Size = UDim2.new(0, 60, 0, 15)
    bindFrame.Position = UDim2.new(0.6000000238418579, 0, 0.23333333432674408, 0)
    bindFrame.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
    bindFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
    bindFrame.BorderSizePixel = 0
    bindFrame.Text = ""
    bindFrame.TextColor3 = Color3.fromRGB(0, 0, 0)
    bindFrame.TextSize = 14
    bindFrame.Font = Enum.Font.SourceSans
    bindFrame.AnchorPoint = Vector2.new(0, 0)
    
    local bindCorner = Instance.new("UICorner")
    bindCorner.CornerRadius = UDim.new(0, 4)
    bindCorner.Parent = bindFrame
    
    local bindStroke = Instance.new("UIStroke")
    bindStroke.Parent = bindFrame
    
    local keyLabel = Instance.new("TextLabel")
    keyLabel.Name = "BindName"
    keyLabel.Size = UDim2.new(0, 59, 0, 13)
    keyLabel.Position = UDim2.new(0, 0, 0, 0)
    keyLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    keyLabel.BackgroundTransparency = 1
    keyLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
    keyLabel.BorderSizePixel = 0
    keyLabel.Text = tostring(defaultKey):gsub("Enum.KeyCode.", "")
    keyLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    keyLabel.TextSize = 12
    keyLabel.Font = Enum.Font.SourceSansBold
    keyLabel.AnchorPoint = Vector2.new(0, 0)
    keyLabel.Parent = bindFrame
    bindFrame.Parent = bindZone
    
    local listening = false
    bindFrame.MouseButton1Click:Connect(function()
        listening = true
        keyLabel.Text = "..."
    end)
    
    UserInputService.InputBegan:Connect(function(input, gameProcessed)
        if gameProcessed then return end
        if listening and input.UserInputType == Enum.UserInputType.Keyboard then
            listening = false
            keyLabel.Text = tostring(input.KeyCode):gsub("Enum.KeyCode.", "")
            if callback then callback(input.KeyCode) end
        end
    end)
    
    bindZone.Parent = section
    return {Zone = bindZone, Key = defaultKey}
end

function Library:CreateInput(section, name, placeholder, callback)
    if not section then return nil end
    
    local yOffset = getNextElementPosition(section)
    
    local inputZone = Instance.new("Frame")
    inputZone.Name = name .. "Zone"
    inputZone.Size = UDim2.new(0, 210, 0, 40)
    inputZone.Position = UDim2.new(0, 0, 0, yOffset)
    inputZone.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    inputZone.BackgroundTransparency = 1
    inputZone.BorderColor3 = Color3.fromRGB(0, 0, 0)
    inputZone.BorderSizePixel = 0
    inputZone.AnchorPoint = Vector2.new(0, 0)
    
    local inputName = Instance.new("TextLabel")
    inputName.Name = "InputName"
    inputName.Size = UDim2.new(0, 79, 0, 14)
    inputName.Position = UDim2.new(0.13734690845012665, 0, -0.008328246884047985, 0)
    inputName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    inputName.BackgroundTransparency = 1
    inputName.BorderColor3 = Color3.fromRGB(0, 0, 0)
    inputName.BorderSizePixel = 0
    inputName.Text = name
    inputName.TextColor3 = Color3.fromRGB(255, 255, 255)
    inputName.TextSize = 10
    inputName.Font = Enum.Font.SourceSans
    inputName.AnchorPoint = Vector2.new(0, 0)
    inputName.Parent = inputZone
    
    local inputFrame = Instance.new("Frame")
    inputFrame.Name = "Input"
    inputFrame.Size = UDim2.new(0, 170, 0, 20)
    inputFrame.Position = UDim2.new(0.09099999815225601, 0, 0.3499999940395355, 0)
    inputFrame.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
    inputFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
    inputFrame.BorderSizePixel = 0
    inputFrame.AnchorPoint = Vector2.new(0, 0)
    
    local inputCorner = Instance.new("UICorner")
    inputCorner.CornerRadius = UDim.new(0, 4)
    inputCorner.Parent = inputFrame
    
    local inputStroke = Instance.new("UIStroke")
    inputStroke.Parent = inputFrame
    
    local inputValue = Instance.new("TextLabel")
    inputValue.Name = "InputValue"
    inputValue.Size = UDim2.new(0, 150, 0, 19)
    inputValue.Position = UDim2.new(0.05882352963089943, 0, 0, 0)
    inputValue.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    inputValue.BackgroundTransparency = 1
    inputValue.BorderColor3 = Color3.fromRGB(0, 0, 0)
    inputValue.BorderSizePixel = 0
    inputValue.Text = placeholder or ""
    inputValue.TextColor3 = Color3.fromRGB(255, 255, 255)
    inputValue.TextSize = 10
    inputValue.Font = Enum.Font.SourceSansBold
    inputValue.AnchorPoint = Vector2.new(0, 0)
    inputValue.Parent = inputFrame
    inputFrame.Parent = inputZone
    
    local line = Instance.new("Frame")
    line.Name = "Line"
    line.Size = UDim2.new(0, 190, 0, 1)
    line.Position = UDim2.new(0.054054055362939835, 0, 0.9937683343887329, 0)
    line.BackgroundColor3 = Color3.fromRGB(161, 161, 161)
    line.BackgroundTransparency = 0.949999988079071
    line.BorderColor3 = Color3.fromRGB(0, 0, 0)
    line.BorderSizePixel = 0
    line.AnchorPoint = Vector2.new(0, 0)
    line.Parent = inputZone
    
    local textBox = Instance.new("TextBox")
    textBox.Size = inputValue.Size
    textBox.Position = inputValue.Position
    textBox.BackgroundTransparency = 1
    textBox.Text = ""
    textBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    textBox.TextSize = 10
    textBox.Font = Enum.Font.SourceSansBold
    textBox.PlaceholderText = placeholder or ""
    textBox.PlaceholderColor3 = Color3.fromRGB(161, 161, 161)
    textBox.ClearTextOnFocus = false
    textBox.Parent = inputFrame
    textBox.Visible = false
    
    inputFrame.MouseButton1Click:Connect(function()
        textBox.Visible = true
        textBox:CaptureFocus()
    end)
    
    textBox.FocusLost:Connect(function(enterPressed)
        textBox.Visible = false
        local text = textBox.Text
        inputValue.Text = text ~= "" and text or placeholder or ""
        if callback then callback(text) end
    end)
    
    inputZone.Parent = section
    return {Zone = inputZone, Value = "", SetText = function(text)
        textBox.Text = text
        inputValue.Text = text ~= "" and text or placeholder or ""
    end, GetText = function() return textBox.Text end}
end

function Library:CreateDropdown(section, name, options, default, callback)
    if not section then return nil end
    
    options = options or {}
    default = default or (options[1] or "")
    
    local yOffset = getNextElementPosition(section)
    
    local dropdownZone = Instance.new("Frame")
    dropdownZone.Name = name .. "Zone"
    dropdownZone.Size = UDim2.new(0, 210, 0, 35)
    dropdownZone.Position = UDim2.new(0, 0, 0, yOffset)
    dropdownZone.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    dropdownZone.BackgroundTransparency = 1
    dropdownZone.BorderColor3 = Color3.fromRGB(0, 0, 0)
    dropdownZone.BorderSizePixel = 0
    dropdownZone.AnchorPoint = Vector2.new(0, 0)
    
    local dropdownName = Instance.new("TextLabel")
    dropdownName.Name = "DropdownName"
    dropdownName.Size = UDim2.new(0, 76, 0, 34)
    dropdownName.Position = UDim2.new(0.0540540405, 0, 0.0114222933, 0)
    dropdownName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    dropdownName.BackgroundTransparency = 1
    dropdownName.BorderColor3 = Color3.fromRGB(0, 0, 0)
    dropdownName.BorderSizePixel = 0
    dropdownName.Text = name
    dropdownName.TextColor3 = Color3.fromRGB(255, 255, 255)
    dropdownName.TextSize = 12
    dropdownName.Font = Enum.Font.SourceSansBold
    dropdownName.AnchorPoint = Vector2.new(0, 0)
    dropdownName.Parent = dropdownZone
    
    local line = Instance.new("Frame")
    line.Name = "Line"
    line.Size = UDim2.new(0, 190, 0, 1)
    line.Position = UDim2.new(0.054054055362939835, 0, 0.9937683343887329, 0)
    line.BackgroundColor3 = Color3.fromRGB(161, 161, 161)
    line.BackgroundTransparency = 0.949999988079071
    line.BorderColor3 = Color3.fromRGB(0, 0, 0)
    line.BorderSizePixel = 0
    line.AnchorPoint = Vector2.new(0, 0)
    line.Parent = dropdownZone
    
    local dropdownButton = Instance.new("Frame")
    dropdownButton.Name = "DropdownButton"
    dropdownButton.Size = UDim2.new(0, 111, 0, 24)
    dropdownButton.Position = UDim2.new(0.422651023, 0, 0.134209335, 0)
    dropdownButton.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
    dropdownButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
    dropdownButton.BorderSizePixel = 0
    dropdownButton.ClipsDescendants = true
    dropdownButton.AnchorPoint = Vector2.new(0, 0)
    
    local buttonCorner = Instance.new("UICorner")
    buttonCorner.CornerRadius = UDim.new(0, 4)
    buttonCorner.Parent = dropdownButton
    
    local selected = Instance.new("TextLabel")
    selected.Name = "Selected"
    selected.Size = UDim2.new(0, 104, 0, 13)
    selected.Position = UDim2.new(0.0558559671, 0, 0.208333328, 0)
    selected.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    selected.BackgroundTransparency = 1
    selected.BorderColor3 = Color3.fromRGB(0, 0, 0)
    selected.BorderSizePixel = 0
    selected.Text = default
    selected.TextColor3 = Color3.fromRGB(255, 255, 255)
    selected.TextSize = 12
    selected.Font = Enum.Font.SourceSansBold
    selected.AnchorPoint = Vector2.new(0, 0)
    selected.Parent = dropdownButton
    
    local blur = Instance.new("Frame")
    blur.Name = "Blur"
    blur.Size = UDim2.new(0, 41, 0, 14)
    blur.Position = UDim2.new(0.641831398, 0, 0.208333328, 0)
    blur.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
    blur.BorderColor3 = Color3.fromRGB(0, 0, 0)
    blur.BorderSizePixel = 0
    blur.AnchorPoint = Vector2.new(0, 0)
    
    local gradient = Instance.new("UIGradient")
    gradient.Transparency = NumberSequence.new{
        NumberSequenceKeypoint.new(0.00, 1.00),
        NumberSequenceKeypoint.new(0.10, 0.75),
        NumberSequenceKeypoint.new(0.38, 0.19),
        NumberSequenceKeypoint.new(0.61, 0.00),
        NumberSequenceKeypoint.new(0.80, 0.02),
        NumberSequenceKeypoint.new(1.00, 0.00)
    }
    gradient.Parent = blur
    blur.Parent = dropdownButton
    
    local arrow = Instance.new("ImageButton")
    arrow.Name = "Arrow"
    arrow.Size = UDim2.new(0, 10, 0, 10)
    arrow.Position = UDim2.new(0.867056608, 0, 0.265472412, 0)
    arrow.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
    arrow.BackgroundTransparency = 1
    arrow.BorderColor3 = Color3.fromRGB(0, 0, 0)
    arrow.BorderSizePixel = 0
    arrow.Image = "rbxassetid://13116390649"
    arrow.AnchorPoint = Vector2.new(0, 0)
    arrow.Parent = dropdownButton
    dropdownButton.Parent = dropdownZone
    
    local opened = false
    local optionsFrame = nil
    
    local function toggleDropdown()
        opened = not opened
        if opened then
            optionsFrame = Instance.new("ScrollingFrame")
            optionsFrame.Size = UDim2.new(0, 111, 0, math.min(#options * 25, 100))
            optionsFrame.Position = UDim2.new(0, 0, 1, 0)
            optionsFrame.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
            optionsFrame.BorderSizePixel = 0
            optionsFrame.ScrollBarThickness = 4
            optionsFrame.CanvasSize = UDim2.new(0, 0, 0, #options * 25)
            
            local optionsCorner = Instance.new("UICorner")
            optionsCorner.CornerRadius = UDim.new(0, 4)
            optionsCorner.Parent = optionsFrame
            
            for i, option in ipairs(options) do
                local optionButton = Instance.new("TextButton")
                optionButton.Size = UDim2.new(1, 0, 0, 25)
                optionButton.Position = UDim2.new(0, 0, 0, (i - 1) * 25)
                optionButton.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
                optionButton.BorderSizePixel = 0
                optionButton.Text = option
                optionButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                optionButton.TextSize = 12
                optionButton.Font = Enum.Font.SourceSansBold
                
                optionButton.MouseButton1Click:Connect(function()
                    selected.Text = option
                    opened = false
                    if optionsFrame then optionsFrame:Destroy() end
                    if callback then callback(option) end
                end)
                
                optionButton.Parent = optionsFrame
            end
            
            optionsFrame.Parent = dropdownButton
        else
            if optionsFrame then optionsFrame:Destroy() end
        end
    end
    
    dropdownButton.MouseButton1Click:Connect(toggleDropdown)
    arrow.MouseButton1Click:Connect(toggleDropdown)
    
    dropdownZone.Parent = section
    return {Zone = dropdownZone, Selected = default, SetSelected = function(value)
        selected.Text = value
        if callback then callback(value) end
    end}
end

_G.Library = Library

return Library
