local hotKeysGui = ScreenGui:FindFirstChild("HotKeys")
local hotKeysMain = hotKeysGui and hotKeysGui:FindFirstChild("MainFrame")
local hotKey1 = hotKeysMain and hotKeysMain:FindFirstChild("HotKey1")
UIData.hotKey1KeyLabel = hotKey1 and hotKey1:FindFirstChild("Key")

local tabNames = {"Combat", "Visual", "Misc", "World", "Settings"}

local function setTab(index)
    if UIData.openedTabLabel then
        UIData.openedTabLabel.Text = tabNames[index] or "Tab " .. tostring(index)
    end

    local function setVisual(btn, active)
        if not btn then return end
        local logo = btn:FindFirstChild("TabLogo")
        local name = btn:FindFirstChild("TabName")
        if active then
            btn.BackgroundTransparency = 0.9
            if logo then logo.ImageColor3 = Color3.fromRGB(150, 17, 255) end
            if name then name.TextColor3 = Color3.fromRGB(150, 17, 255) end
        else
            btn.BackgroundTransparency = 1
            if logo then logo.ImageColor3 = Color3.fromRGB(161, 161, 161) end
            if name then name.TextColor3 = Color3.fromRGB(161, 161, 161) end
        end
    end

    for i = 1, 5 do
        setVisual(UIData.tabs[i], index == i)
    end
end

setTab(1)

for i = 1, 5 do
    if UIData.tabs[i] then
        UIData.tabs[i].MouseButton1Click:Connect(function() setTab(i) end)
    end
end

